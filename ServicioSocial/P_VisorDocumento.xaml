<Window x:Class="ServicioSocial.P_VisorDocumento"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ServicioSocial"
        mc:Ignorable="d"
        Title="Visor de documento" Height="800" Width="640">
    <Grid>
        <TextBlock x:Name="textblockDocumentName" Margin="10,10,0,0" TextWrapping="Wrap" VerticalAlignment="Top" FontSize="26" HorizontalAlignment="Left" Width="614"/>
        <DocumentViewer x:Name="DocumentVisor" HorizontalAlignment="Left" Margin="10,50,0,0" VerticalAlignment="Top"/>

    </Grid>
</Window>
